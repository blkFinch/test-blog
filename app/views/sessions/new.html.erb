<h2>Login</h2>
<%= form_for(:session, url: login_path) do |form| %>
  <p>
  <%= form.label :screen_name %>
  <%= form.text_field :screen_name %>
</p>
<p>
  <%= form.label :password %>
  <%= form.text_field :password %>
</p>
<%= form.submit "Log In" %>
<% end %>
<br>
<em><%= link_to 'Sign Up!', new_user_path %></em>
